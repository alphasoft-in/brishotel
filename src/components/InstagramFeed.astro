---
const images = [
  	"/it1.png",
  	"/it2.png",
  	"/it3.png",
  	"/it4.png",
  	"/it5.png",
];
---
<section class="py-10 bg-white">
  	<div class="max-w-7xl mx-auto text-center">
    	<h3 class="text-lg font-semibold mb-6">
      		<a href="https://www.instagram.com/brishotel/" target="_blank" rel="noopener noreferrer" class="inline-flex items-center merriweather gap-2 text-[#5a3d37] hover:text-[#759120] transition-colors">
        		<i class="fa-brands fa-instagram text-xl"></i>
        		BRISHOTEL
      		</a>
    	</h3>

    	<div class="flex justify-center gap-4 flex-wrap">
      		{images.map((src) => (
        		<img src={src} alt="Instagram photo" class="w-48 h-48 object-cover rounded-lg shadow-md hover:scale-105 transition-transform duration-300" />
      		))}
    	</div>
  	</div>
</section>