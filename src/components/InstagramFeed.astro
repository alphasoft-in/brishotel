---
const images = [
  "/it1.png",
  "/it2.png",
  "/it3.png",
  "/it4.png",
  "/it5.png",
];
---

<section class="py-10 bg-white">
  <div class="max-w-7xl mx-auto text-center">
    <h3 class="text-lg font-semibold mb-6">
      <span class="inline-flex items-center gap-2 text-[#b76e79]">
        <i class="ri-instagram-line text-xl"></i> @BRISHOTEL
      </span>
    </h3>
    <div class="flex justify-center gap-4 flex-wrap">
      {images.map((src) => (
        <img src={src} alt="Instagram photo" class="w-48 h-48 object-cover" />
      ))}
    </div>
  </div>
</section>
